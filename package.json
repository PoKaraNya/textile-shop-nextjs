{
  "name": "textile-shop",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prod": "env-cmd -f .env.prod next dev",
    "dev": "env-cmd -f .env.dev next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "db:generate": "env-cmd -f .env.dev prisma generate",
    "db:migrate": "env-cmd -f .env.dev prisma migrate dev",
    "db:migrate:prod": "env-cmd -f .env.prod prisma migrate dev",
    "db:studio": "env-cmd -f .env.dev prisma studio",
    "db:studio:prod": "env-cmd -f .env.prod prisma studio",
    "db:seed": "env-cmd -f .env.dev ts-node prisma/seed.ts",
    "db:seed:prod": "env-cmd -f .env.prod ts-node prisma/seed.ts",
    "db:seed:hard": "pnpm run db:reset && pnpm run db:seed",
    "db:reset": "env-cmd -f .env.dev ts-node prisma/db-reset.ts",
    "db:reset:prod": "env-cmd -f .env.prod ts-node prisma/db-reset.ts",
    "db:push": "env-cmd -f .env.dev prisma db push",
    "db:push:prod": "env-cmd -f .env.prod prisma db push",
    "prepare": "husky && husky install",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage --silent"
  },
  "dependencies": {
    "@auth/core": "^0.28.2",
    "@auth/prisma-adapter": "^1.5.2",
    "@prisma/client": "^5.12.1",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@t3-oss/env-nextjs": "^0.9.2",
    "@tanstack/react-query": "^5.29.0",
    "axios": "^1.6.8",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "env-cmd": "^10.1.0",
    "eslint-config-airbnb": "^19.0.4",
    "lucide-react": "^0.364.0",
    "next": "14.1.4",
    "next-auth": "^4.24.7",
    "next-themes": "^0.3.0",
    "react": "^18",
    "react-dom": "^18",
    "resend": "^3.2.0",
    "sonner": "^1.4.41",
    "tailwind-merge": "^2.2.2",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@testing-library/jest-dom": "6.1.5",
    "@testing-library/react": "14.1.2",
    "@types/jest": "29.5.11",
    "@types/node": "^20.12.4",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-next": "14.1.4",
    "husky": "^8.0.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "postcss": "^8",
    "prisma": "^5.12.1",
    "tailwindcss": "^3.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.4",
    "zod-prisma": "^0.5.4"
  }
}
